{
  "name": "as-music",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:coverageAll": "jest --coverage --collectCoverageFrom='**/*.{ts,tsx}' && '!**/dist/**'",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "lint": "eslint \"./**/*.{ts,tsx,js,jsx}\" --quiet",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx}\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --watch server --ext ts --exec \"ts-node src/server/index.ts\"",
    "client": "webpack-dev-server --config webpack.config.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.config.ts",
    "build:server": "webpack --config webpack.config.ts"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js",
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "keywords": [],
  "author": "EdSheranaway",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.9.2",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.8.1",
    "sass": "^1.58.2"
  }
}
